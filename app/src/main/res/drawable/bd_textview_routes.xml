<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <!--Layer list for an array of drawables in a drawable object-->
    <item>
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/colorOrange"
                />
            <corners
                android:radius="10dp"
                />
            <padding
                android:top="3dp"
                android:bottom="3dp"
                android:left="3dp"
                android:right="3dp"
                />
        </shape>
    </item>
</layer-list>
